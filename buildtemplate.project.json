{
  "name": "Master-Architect",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Common": {
        "$path": "buildtemplate/shared"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "buildtemplate/ServerScripts"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "buildtemplate/ClientScripts"
        }
      },
      "StarterCharacterScripts": {
        "CharacterScripts": {
          "$path": "buildtemplate/ClientScripts/CharacterScripts"
        }
      }
    },

    "Lighting": {
      "$properties": {
        "Ambient": [0, 0, 0],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
